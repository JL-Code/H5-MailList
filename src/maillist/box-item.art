{{each Users}}
<div class="cc-item">
    <div class="z-avatar">
        {{if $value.Avatar}}
        <img src="{{$value.Avatar}}" class="z-avatar__img">
        {{else if !$value.Avatar}}
        <img src="data:image/png;base64,"
            class="z-avatar__img">
        {{/if}}
        <span class="z-avatar__text">{{$value.Name}}</span>
    </div>
    <a class="cc-close" data-id="{{$value.ID}}" href="javascript:;"></a>
</div>
{{/each}}
<a href="javascript:;" class="cc-item cc-add"></a>